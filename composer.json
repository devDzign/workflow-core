{
  "name": "inter-invest/workflow-core",
  "version": "0.0.2",
  "type": "symfony-bundle",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "InterInvest\\Workflow\\": "/"
    }
  },
  "authors": [
    {
      "name": "Mourad Chabour",
      "email": "mourad.chabour@services.inter-invest.fr"
    }
  ],
  "require": {
    "php": ">=8.3",
    "symfony/http-kernel": "^7.1",
    "symfony/messenger": "^7.1",
    "symfony/dependency-injection": "^7.1",
    "symfony/config": "^7.1",
    "symfony/serializer": "^7.1"
  },
  "extra": {
    "branch-alias": {
      "dev-main": "dev-main"
    },
    "symfony": {
      "recipe": {
        "repo": "repo-name",
        "branch": "main",
        "type": "flex-recipe"
      }
    }
  },
  "config": {
    "allow-plugins": {
      "symfony/flex": true
    }
  }
}
